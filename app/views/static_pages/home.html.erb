<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= render "shared/user_info" %>
      </section>
      <section class="stats">
        <%= render "shared/stats" %>
      </section>
    </aside>
  </div>
<% else %>
  <% provide :title, t("static_pages.home") %>
  <h1><%= t "static_pages.home" %></h1>
<% end %>
